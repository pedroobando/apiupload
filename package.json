{
  "name": "apiupload",
  "version": "0.3.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "clean": "rm -rf dist",
    "debug": "nodemon --exec babel-node --inspect-brk src/index.js ",
    "serve": "node dist/index.js NODE_ENV=PROD",
    "start": "nodemon src/index.js NODE_ENV=DEV --exec babel-node",
    "test": "jest --runInBand --verbose",
    "coverage": "jest --coverage --runInBand --verbose",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\"",
    "lint": "eslint src --fix",
    "release": "npm version patch && git push && git push --tags"
  },
  "keywords": [
    "express",
    "babel",
    "nodemon",
    "mongodb",
    "mongo",
    "multer",
    "fs-extra"
  ],
  "author": {
    "name": "Pedro Obando",
    "email": "pedro.obando@gmail.com"
  },
  "description": "Application to upload files to a repository, using mongo database to save the registry",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "fs-extra": "^9.0.0",
    "mongodb": "^3.6.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1"
  }
}
